{
  "name": "dosukebe-ranking-pxv",
  "version": "1.0.0",
  "private": true,
  "description": "Dosukebe Ranking PXV System",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "docker-compose up",
